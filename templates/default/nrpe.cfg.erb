command[check_jmx_dc_mr_total_events_routed]=<%= node['nagios']['plugin_dir'] %>/check_jmx -U service:jmx:rmi:///jndi/rmi://localhost:9001/jmxrmi -O "message_router:type=MessageRouter" -A TotalEventsRouted

