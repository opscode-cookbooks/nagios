command[check_jmx_dc_qa_total_queries_received]=<%= node['nagios']['plugin_dir'] %>/check_jmx -U service:jmx:rmi:///jndi/rmi://localhost:9001/jmxrmi -O "query_aggregator:type=QueryAggregator" -A TotalQueriesReceived

command[check_jmx_dc_qa_total_query_results_received]=<%= node['nagios']['plugin_dir'] %>/check_jmx -U service:jmx:rmi:///jndi/rmi://localhost:9001/jmxrmi -O "query_aggregator:type=QueryAggregator" -A TotalQueryResultsReceived

